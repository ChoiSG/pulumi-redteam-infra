# AWS Credentials
AWS_ACCESS_KEY=
AWS_SECRET_KEY=
AWS_REGION=ap-northeast-2

# CloudFront Distribution Configuration
CLOUDFRONT_DISTRIBUTION_NAME=cdn-redirector-pulumi
CLOUDFRONT_COMMENT=Pulumi-cloudfront-redirector

# Redirector Origin Configuration - Redirector Domain 
REDIRECTOR_DOMAIN=fonts.example.com

# Price Class - determines which edge locations to use
# Options: PriceClass_All, PriceClass_200, PriceClass_100
# PriceClass_100 = US, Canada, Europe (cheapest)
# PriceClass_200 = PriceClass_100 + Asia, Middle East, Africa
# PriceClass_All = All edge locations (most expensive)
CLOUDFRONT_PRICE_CLASS=PriceClass_100
